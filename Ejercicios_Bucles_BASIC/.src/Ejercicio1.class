Public Sub Button1_Click()

  Dim numero As Integer
  Dim i As Integer
  Dim multiplo As Integer
  
  numero = Teclado.Text
  
  For i = 1 To 10 Step 1
  
    multiplo = numero * i
    
    LResultado.Text = (LResultado.Text & multiplo)
  
  Next 
  
End
